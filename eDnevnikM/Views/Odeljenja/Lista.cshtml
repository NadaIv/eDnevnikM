@using eDnevnikM.Models;

@model IEnumerable<Odeljenja>

@{
    ViewBag.Title = "Odeljenja List";
}

<div style="font-family:Arial">
    <h2>Odeljenja List</h2>

    <ul>
        @foreach (Odeljenja odeljenja in Model)
        {
            <li>
                @Html.ActionLink(odeljenja.MatBrOdeljenja, "Lista","Ucenici", new { odeljenjeId = odeljenja.OdeljenjeID },null)

            </li>

        }
</ul>
</div>

